{
  "builds": [
    {"src": "index.html", "use": "@vercel/static"},
    {"src": "api/*.js", "use": "@vercel/node"}
  ],
  "routes": [
    {"src": "/api/(.*)", "dest": "/api/$1"},
    {"src": "/creative-ideas.json", "dest": "/creative-ideas.json"},
    {"src": "/robots.txt", "dest": "/robots.txt"},
    {"src": "/sitemap.xml", "dest": "/sitemap.xml"},
    {"src": "/site.webmanifest", "dest": "/site.webmanifest"},
    {"src": "/stats.html", "dest": "/stats.html"},
    {"src": "/(.*)\\.(png|jpg|jpeg|gif|svg|ico|css|js|json|txt|xml)$", "dest": "/$1.$2"},
    {"src": "/", "dest": "/index.html"},
    {"src": "/(.*)","dest": "/index.html"}
  ]
}
